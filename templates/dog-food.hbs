{{#each dog_brands}}
<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title"><b>{{brand}}</b></h3>
    </div>
    <div class="panel-body">
      <table class="table table-striped table-hover">
		    <thead>
	        <tr>
            <th>Type of Food</th>
            <th>Bag Size</th>
            <th>Price</th>
	        </tr>
		    </thead>
		    <tbody>
	    		{{#each types as |theType|}}
	    			{{#each volumes}}
			        <tr class="info">
		            <td>{{theType.name}}</td>
		            <td>{{size}}</td>
		            <td>${{price}}</td>
			        </tr>
	    			{{/each}}
        {{/each}}
        </tbody>
      </table>
    </div>
</div>
{{/each}}