{{!--This is the layout that I would like to have. It's currently broken--}}
{{#each dog_brands}}
<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title">{{name}}</h3>
    </div>
    <div class="panel-body">
        <table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>Type of Food</th>
            <th>Bag Size</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
    		{{#each types}}
        <tr class="info">
            <td>{{type}}</td>
            {{#each volumes}}
            <td>{{name}}</td>
            <td>{{price}}</td>
            {{/each}}
        </tr>
        {{/each}}
    </div>
</div>
{{/each}}



